@page "/"
@namespace Outlook.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>HenE_Ej2.blazor_syncfusion</title>
    <base href="~/" />
    <link href="_content/Syncfusion.Blazor/styles/bootstrap4.css" rel="stylesheet" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="https://cdn.syncfusion.com/ej2/18.1.0.36/material.css" rel="stylesheet" />
    <script src="https://github.com/Daddoon/Blazor.Polyfill/releases/download/3.0.1/blazor.polyfill.min.js"></script>
    <script src="https://cdn.syncfusion.com/blazor/18.1.36/syncfusion-blazor.min.js"></script>
    <script src="https://cdn.syncfusion.com/ej2/18.1.0.36/dist/ej2.min.js"></script>
    <script src="https://cdn.syncfusion.com/ej2/18.1.0.36/dist/ejs.interop.min.js"></script>
    <script src="_framework/blazor.server.js"></script>
    <script src="~/scrollto.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Princess+Sofia&display=swap" rel="stylesheet">
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
